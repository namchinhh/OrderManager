<config	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_OrderManager::config_ordermanager" title=" OrderManager Setting Section"/>
                        </resource>
                    </resource>
                </resource>
                <resource	id="Magento_Sales::sales">
                    <resource	id="Magenest_OrderManager::ordermanager" title="Manage OrderManager" sortOrder="60">
                        <resource	id="Magenest_OrderManager::order"	title="Order Manager"	/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>